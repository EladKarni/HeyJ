{
  "expo": {
    "name": "HeyJ",
    "slug": "HeyJ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "heyj",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio", "remote-notification"],
        "NSCameraUsageDescription": "Allow this app to access camera.",
        "NSMicrophoneUsageDescription": "Allow this app to access your microphone",
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": {
        "policy": "sdkVersion"
      },
      "bundleIdentifier": "com.ekarni.heyj",
      "splash": {
        "backgroundColor": "#FFFFFF",
        "image": "./assets/splash.png"
      },
      "entitlements": {
        "com.apple.developer.applesignin": ["Default", "Default"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "package": "com.ekarni.heyj",
      "runtimeVersion": "1.0.0",
      "splash": {
        "backgroundColor": "#FFFFFF",
        "image": "./assets/splash.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "production"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos."
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "425b4502-a4b9-4fe6-a2ea-ca4129daa0dc"
      },
      "oneSignalAppId": "1d79c718-998c-49d0-89cb-37c5ea140309",
      "oneSignalRestApiKey": "os_v2_app_dv44ogezrre5bcolg7c6ufadbhwkfoz2n2yuy65jzj7jjsmxdjmkurtdtjngab2xmhdqpwtvy6oxcwizh2o7w6szfdedgfdjcrisw2a"
    },
    "jsEngine": "hermes"
  }
}
