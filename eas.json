{
  "cli": {
    "version": ">= 5.9.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "env": {
        "NODE_ENV": "development"
      },
      "prebuildCommand": "./scripts/eas-prebuild.sh"
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release",
        "enterpriseProvisioning": "adhoc"
      },
      "prebuildCommand": "./scripts/eas-prebuild.sh"
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "environment": "production",
      "prebuildCommand": "./scripts/eas-prebuild.sh"
    },
    "simulator": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "prebuildCommand": "./scripts/eas-prebuild.sh"
    }
  },
  "submit": {
    "production": {}
  }
}
